<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_main">

    <ImageView
        android:background="@drawable/ksre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/krseLogo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25.0dp"
        android:layout_marginBottom="0.0dp"
        android:layout_marginLeft="0.0dp" />
    <SearchView
        android:background="@drawable/search"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="150.0dp"
        android:layout_centerHorizontal="true"
        android:layout_marginHorizontal="10.0dp"
        android:id="@+id/search"
        android:iconifiedByDefault="false"
        android:queryBackground="@android:color/transparent"
        android:longClickable="false" />
    <LinearLayout
        android:id="@+id/leftCol"
        android:layout_marginTop="230.0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:rowCount="3"
        android:columnCount="2"
        android:layout_marginRight="185dp"
        android:layout_marginLeft="5dp"
        android:orientation="vertical">
        <Button
            android:text="Baked Goods/Prepared Foods"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "0"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:id="@+id/firstButton" />
        <Button
            android:text="Drinks"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "1"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/thirdButton" />
        <Button
            android:text="Refridgerated/Frozen Processed Foods"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "2"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/fifthButton" />
        <Button
            android:text="Other-Sampling of Foods Products"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "2"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/seventhButton" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/rightCol"
        android:layout_marginTop="230.0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:rowCount="3"
        android:columnCount="2"
        android:layout_marginLeft="185dp"
        android:layout_marginRight="5dp"
        android:orientation="vertical">
        <Button
            android:text="Jams, Jellies, and Shelf-Stable Canned Foods"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "0"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:id="@+id/secondButton" />
        <Button
            android:text="Meat, Eggs, and Dairy Products"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "1"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/fourthButton" />
        <Button
            android:text="Fresh Produce"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "2"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/sixthButton" />
        <Button
            android:text="Other Food Products"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "2"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/eighthButton" />
    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/firstClick"
        android:layout_marginTop="230.0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_marginBottom="140dp"
        android:orientation="vertical">
        <Button
            android:text="Baked Goods"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "0"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:id="@+id/bakedButton" />
        <Button
            android:text="Prepared Foods"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "1"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/preparedButton" />
        <Button
            android:text="Immediate Consumption"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "2"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/imConsumpButton" />
    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/thirdClick"
        android:layout_marginTop="230.0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_marginBottom="200dp"
        android:orientation="vertical">
        <Button
            android:text="Alcoholic"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "0"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:id="@+id/alcButton" />
        <Button
            android:text="Non-Alcoholic"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "1"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/nonAlcButton" />
    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/fifthClick"
        android:layout_marginTop="230.0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_marginBottom="200dp"
        android:orientation="vertical">
        <Button
            android:text="Refridgerated Foods"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "0"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:id="@+id/refridgerateButton" />
        <Button
            android:text="Processed Foods"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "1"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/processedButton" />
    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/secondClick"
        android:layout_marginTop="230.0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_marginBottom="140dp"
        android:orientation="vertical">
        <Button
            android:text="Jams"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "0"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:id="@+id/jamButton" />
        <Button
            android:text="Jellies"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "1"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/jellyButton" />
        <Button
            android:text="Shelf Stable Canned Foods"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "2"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/shelfStableButton" />
    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/fourthClick"
        android:layout_marginTop="230.0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_marginBottom="140dp"
        android:orientation="vertical">
        <Button
            android:text="Meats"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "0"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:id="@+id/meatButton" />
        <Button
            android:text="Eggs"
            android:textColor="@android:color/white"
            android:background="@drawable/button"
            android:textSize="13dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "1"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/eggsButton" />
        <Button
            android:text="Dairy Products"
            android:textColor="@android:color/white"
            android:textSize="13dp"
            android:background="@drawable/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row = "2"
            android:layout_column = "0"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:id="@+id/dairyButton" />
    </LinearLayout>
    <Button
        android:text="Back"
        android:layout_marginTop="480dp"
        android:textColor="@android:color/white"
        android:background="@drawable/button"
        android:textSize="25dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row = "0"
        android:layout_column = "0"
        android:layout_centerHorizontal="true"
        android:id="@+id/backButton" />
</RelativeLayout>
